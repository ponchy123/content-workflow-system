NODE_ENV=development

# 开发环境配置
VITE_APP_TITLE=物流运费订单计算系统
VITE_API_BASE_URL=https://freight.test:8000
VITE_WS_BASE_URL=wss://freight.test:8000
VITE_APP_BASE_URL=/

# API前缀
VITE_APP_BASE_API=/api/v1

# API超时设置（毫秒）
VITE_API_TIMEOUT=15000

# 是否启用Mock数据
VITE_ENABLE_MOCK=false

# 是否启用调试工具
VITE_APP_DEBUG=true

# 上传文件大小限制（MB）
VITE_FILE_UPLOAD_SIZE=10485760

# 安全配置
VITE_ENABLE_HTTPS=true
VITE_ENCRYPTION_KEY=your-development-encryption-key